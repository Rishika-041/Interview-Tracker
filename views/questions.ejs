<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Practise</title>
  <link rel="stylesheet" href="/assets2/styles.css">
</head>
<body>
  <nav>
    <h1><a href="/">Interview Tracker</a></h1>
    <ul>
      <% if (user) { %>
        <li>Welcome, <%= user.email %></li>
        <li><a href="/logout">Log out</a></li>
      <% } else { %>
        <li><a href="/login">Log in</a></li>
        <li><a href="/signup" class="btn">Sign up</a></li>
      <% } %>

      <li><a href= "/">Home</a></li>
    </ul>
  </nav>

<ul class="recipes">

  <% b.forEach(function(value){ %>
    <% if(value.approved){ %>
  <li class="recipe">
    <img src="https://scontent.fdel58-1.fna.fbcdn.net/v/t1.6435-9/143902896_1594415774081848_4870013509024203251_n.jpg?_nc_cat=105&ccb=1-3&_nc_sid=09cbfe&_nc_ohc=BOywjNIFohwAX_uFcKS&_nc_ht=scontent.fdel58-1.fna&oh=384fd21e4dc98dea18f455aef664d705&oe=60E878E1" alt="<%=value.topic%>">
    <a href="/<%= value.link %>"><h4><%=value.name%></h4></a>
    <p></p>
  </li>
  <%  } %> 
  <% })%>
  
</ul>
<br>
<br>
<br>
<br>
<br>
<br>
<br>
<div> 

  <h4 class="compiler"><a href="https://www.onlinegdb.com/online_c++_compiler"> Open compiler</a></h4>

</div>

<footer>Copyright 2021 InterviewTracker</footer>

</body>
</html>